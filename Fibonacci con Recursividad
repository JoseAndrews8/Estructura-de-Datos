#include <iostream>

using namespace std;

int fibonacci(int n);

int main()
{
    int n;
    
    cout << "-----------Fibonacci-----------" << endl;
    cout << "Ingresar el numero: ";
    cin >> n;

    cout << "El resultado es: " << fibonacci(n);

    return 0;
}

int fibonacci(int n){
    if (n <= 1)
    {
        return n;
    }
    else 
        return fibonacci (n - 1) + fibonacci(n - 2); 
}
